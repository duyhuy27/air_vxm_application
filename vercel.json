{
  "version": 2,
  "buildCommand": "cd frontend-react && npm ci && npm run build",
  "outputDirectory": "frontend-react/dist",
  "framework": "vite",
  "nodejs": "18.x",
  "env": {
    "VITE_API_BASE_URL": "https://fastapi-bigquery-app-production.up.railway.app/api/v1",
    "NODE_ENV": "production"
  },
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}